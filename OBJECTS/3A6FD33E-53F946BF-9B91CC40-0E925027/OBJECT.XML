<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.82.0.17.PUB" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">Blackjack</name>
		<description type="string">kDesc</description>
		<help_text type="string"></help_text>
		<author type="string">David Foster, Kevin Strype</author>
		<object_version type="string">1.0</object_version>
		<company type="string">Digital Lesiure</company>
		<template type="string"></template>
		<template_version type="string"></template_version>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">13</minimum_age>
				<parental_control_level type="int">5</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
		<LPID type="string"></LPID>
	</header>
	<lua_environment>
		<script type="string">FrontierBlackjack</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string">OnListenerReceivedData()</on_listener_received_data>
		<on_destroy type="string"></on_destroy>
		<on_clothing_change type="string"></on_clothing_change>
		<senders type="list" />
	</lua_environment>
	<targetable>
		<interactive type="bool">false</interactive>
		<on_activated type="string"></on_activated>
		<on_targeted type="string"></on_targeted>
		<is_targetable_by_remote_people type="bool">false</is_targetable_by_remote_people>
		<trigger_radius type="float">-1</trigger_radius>
		<can_be_targeted_while_seated type="bool">false</can_be_targeted_while_seated>
	</targetable>
	<entity />
	<network>
		<definition type="string">Network.xml</definition>
		<on_bag_created type="string"></on_bag_created>
		<on_bag_deleted type="string"></on_bag_deleted>
		<on_message_received type="string">onReceivedMessage()</on_message_received>
		<auto_replicate type="bool">false</auto_replicate>
	</network>
	<osd>
		<types type="string"></types>
	</osd>
	<mini_game>
		<on_update type="string">onUpdate()</on_update>
		<on_send_welcome_message type="string">onSendWelcomeMessage()</on_send_welcome_message>
		<on_render type="string">onRender()</on_render>
		<on_can_local_player_join type="string">onCanLocalPlayerJoin()</on_can_local_player_join>
		<on_add_player type="string">onAddPlayer()</on_add_player>
		<on_remove_player type="string">onRemovePlayer()</on_remove_player>
		<on_local_player_join type="string">onLocalPlayerJoin()</on_local_player_join>
		<on_local_player_leave type="string">onLocalPlayerLeave()</on_local_player_leave>
		<on_local_player_update_gameplay type="string">onLocalPlayerUpdateGameplay()</on_local_player_update_gameplay>
		<join_type type="string">minimal</join_type>
		<is_allowed_label_change type="bool">false</is_allowed_label_change>
		<is_allowed_chat_bubble_change type="bool">false</is_allowed_chat_bubble_change>
		<session_size type="int">5</session_size>
		<network_session type="bool">true</network_session>
		<on_custom_exit_menu_option_selected type="string"></on_custom_exit_menu_option_selected>
		<is_allowed_presence_broadcast type="bool">false</is_allowed_presence_broadcast>
	</mini_game>
	<pad />
	<renderer />
	<camera />
	<repertoire>
		<is_exclusive type="bool">false</is_exclusive>
		<target_rigs type="list">
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000000</rig>
				<repertoire_resource type="string">Male_Rep</repertoire_resource>
				<emote_resource type="string">Male_Emo</emote_resource>
				<label type="string">blackjackMRepertoire</label>
			</target>
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000001</rig>
				<repertoire_resource type="string">Female_Rep</repertoire_resource>
				<emote_resource type="string">Female_Emo</emote_resource>
				<label type="string">blackjackFRepertoire</label>
			</target>
		</target_rigs>
		<anim_registers type="bag">
			<max_players type="int">0</max_players>
			<string_registers type="list" />
			<bool_registers type="list" />
			<number_registers type="list" />
			<integer_registers type="list" />
		</anim_registers>
	</repertoire>
	<embedded_object>
		<on_update type="string"></on_update>
		<string_parameters type="list">
			<parameter type="bag">
				<name type="string">tablemodel</name>
				<default_value type="string">BlackJackTable.mdl</default_value>
			</parameter>
		</string_parameters>
		<boolean_parameters type="list" />
		<numeric_parameters type="list">
			<parameter type="bag">
				<name type="string">minbet</name>
				<default_value type="float">25</default_value>
				<min_value type="string">0</min_value>
				<max_value type="string">1000000</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">maxbet</name>
				<default_value type="float">100</default_value>
				<min_value type="string">0</min_value>
				<max_value type="string">1000000</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">leaderboardx</name>
				<default_value type="float">0</default_value>
				<min_value type="string">-100000</min_value>
				<max_value type="string">100000</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">leaderboardy</name>
				<default_value type="float">0</default_value>
				<min_value type="string">-100000</min_value>
				<max_value type="string">100000</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">leaderboardz</name>
				<default_value type="float">0</default_value>
				<min_value type="string">-100000</min_value>
				<max_value type="string">100000</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">leaderboardrot</name>
				<default_value type="float">0</default_value>
				<min_value type="string">0</min_value>
				<max_value type="string">360</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">table_number</name>
				<default_value type="float">1</default_value>
				<min_value type="string">1</min_value>
				<max_value type="string">1000</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">scene_type</name>
				<default_value type="float">2</default_value>
				<min_value type="string">1</min_value>
				<max_value type="string">1000</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">decks</name>
				<default_value type="float">5</default_value>
				<min_value type="string">1</min_value>
				<max_value type="string">5</max_value>
			</parameter>
		</numeric_parameters>
		<enum_parameters type="list" />
	</embedded_object>
</objectdef>