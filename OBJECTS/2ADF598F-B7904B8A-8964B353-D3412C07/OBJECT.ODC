  <name lang="de-DE" default="false">Verrückter Wissenschaftler Anfängerset</name>
  <name lang="it-IT" default="false">Kit Iniziale dello Scienziato Pazzo</name>
  <name lang="es-ES" default="false">Juego de iniciación de Científico Loco</name>
