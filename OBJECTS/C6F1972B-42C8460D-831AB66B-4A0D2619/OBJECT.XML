<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://home.scedev.net/schema/object">
  <header>
    <name type="string">Title</name>
    <description type="string">Desc</description>
    <author type="string" />
    <object_version type="string" />
    <company type="string">Outso Ltd.</company>
    <legal type="list">
      <age_rating type="bag" />
    </legal>
  </header>
  <lua_environment>
    <script type="string">SingStarJukeBox.lua</script>
  </lua_environment>
  <targetable>
    <on_activated type="string" />
    <on_targeted type="string" />
    <is_targetable_by_remote_people type="bool" />
  </targetable>
  <entity />
  <network>
    <definition type="string" />
    <on_bag_created type="string" />
    <on_bag_deleted type="string" />
    <on_message_received type="string">JukeBox.OnMessageRecieved()</on_message_received>
    <auto_replicate type="bool" />
  </network>
  <mini_game>
    <on_update type="string">JukeBox.Update()</on_update>
    <on_send_welcome_message type="string" />
    <on_render type="string">JukeBox.Render()</on_render>
    <on_can_local_player_join type="string">JukeBox.OnCanLocalPlayerJoin()</on_can_local_player_join>
    <on_add_player type="string" />
    <on_remove_player type="string" />
    <on_local_player_join type="string">JukeBox.OnLocalPlayerJoin()</on_local_player_join>
    <on_local_player_leave type="string">JukeBox.OnLocalPlayerLeave()</on_local_player_leave>
    <on_local_player_update_gameplay type="string">JukeBox.UpdateLocalPlayer()</on_local_player_update_gameplay>
    <join_type type="string">normal</join_type>
  </mini_game>
  <screen />
  <pad />
  <renderer />
  <camera />
  <reward />
</objectdef>