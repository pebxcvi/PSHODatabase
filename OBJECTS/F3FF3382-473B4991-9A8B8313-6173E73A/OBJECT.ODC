  <name lang="en-GB" default="true">400 000 German Facebook Fans t-shirt</name>
  <name lang="fr-FR" default="false">T-shirt pour les 400 000 fans Facebook allemands</name>
  <name lang="de-DE" default="false">400 000 deutsche Facebook Fans T-Shirt</name>
