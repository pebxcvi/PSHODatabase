<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.3.6.22.PUB" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<author type="string">seanr</author>
		<object_version type="string">1.0</object_version>
		<company type="string">Mass Media</company>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string">PEGI</rating_system>
				<rating type="string">3+</rating>
				<minimum_age type="int">3</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string">disco shirt</additional_data>
			</age_rating>
		</legal>
	</header>
	<clothing>
		<name type="string">SW_shrt</name>
		<type type="string">Tors</type>
		<models type="list">
			<model type="bag">
				<name type="string">SW_shrt</name>
				<lods type="bag">
					<lod1 type="string">SW_shrt_lod1.mdl</lod1>
					<lod2 type="string">SW_shrt_lod2.mdl</lod2>
					<lod3 type="string">SW_shrt_lod3.mdl</lod3>
				</lods>
				<skeleton type="string"></skeleton>
				<materials type="list">
					<material type="bag">
						<type type="string">default</type>
						<shadingGroup type="string">ATGMaterial31SG</shadingGroup>
						<path type="string"></path>
						<maps type="list">
							<map type="bag">
								<name type="string">colourMap</name>
								<source type="string">SW_shrt.ATGMaterial31SG.colourMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">normalMap</name>
								<source type="string">SW_shrt.ATGMaterial31SG.normalMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">specularMap</name>
								<source type="string">SW_shrt.ATGMaterial31SG.specularMap</source>
								<format type="string">resource</format>
							</map>
						</maps>
					</material>
					<material type="bag">
						<type type="string">skin</type>
						<shadingGroup type="string">bodySG</shadingGroup>
						<path type="string"></path>
						<maps type="list">
							<map type="bag">
								<name type="string">colourMap</name>
								<source type="string">MaleSkinBodyColour</source>
								<format type="string">generated</format>
							</map>
							<map type="bag">
								<name type="string">normalMap</name>
								<source type="string">SW_shrt.bodySG.normalMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">specularMap</name>
								<source type="string">MaleSkinBodySpecular</source>
								<format type="string">generated</format>
							</map>
						</maps>
					</material>
					<material type="bag">
						<type type="string">default</type>
						<shadingGroup type="string">ATGMaterial29SG</shadingGroup>
						<path type="string"></path>
						<maps type="list">
							<map type="bag">
								<name type="string">colourMap</name>
								<source type="string">SW_shrt.ATGMaterial29SG.colourMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">normalMap</name>
								<source type="string">SW_shrt.ATGMaterial29SG.normalMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">specularMap</name>
								<source type="string">SW_shrt.ATGMaterial29SG.specularMap</source>
								<format type="string">resource</format>
							</map>
						</maps>
					</material>
					<material type="bag">
						<type type="string">hair</type>
						<shadingGroup type="string">hairSG</shadingGroup>
						<path type="string"></path>
						<maps type="list">
							<map type="bag">
								<name type="string">colourMap</name>
								<source type="string">SW_shrt.hairSG.colourMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">alphaMap</name>
								<source type="string">SW_shrt.hairSG.alphaMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">specularMap</name>
								<source type="string">SW_shrt.hairSG.specularMap</source>
								<format type="string">resource</format>
							</map>
						</maps>
					</material>
