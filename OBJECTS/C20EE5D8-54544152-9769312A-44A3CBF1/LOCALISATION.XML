    <LANG ID="ko-KR">간부석</LANG>
    <LANG ID="en-SG">Leaders' Seats</LANG>
    <LANG ID="zh-HK">幹部座位</LANG>
    <LANG ID="zh-TW">幹部座位</LANG>
