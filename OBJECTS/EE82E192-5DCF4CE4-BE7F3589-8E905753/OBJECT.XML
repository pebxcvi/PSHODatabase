<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">Gothic Top Hat</name>
		<description type="string">For darkly dreaming dandies.</description>
		<author type="string">Joseph</author>
		<object_version type="string">1.0</object_version>
		<company type="string" />
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string">ESRB</rating_system>
				<rating type="string">E</rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">0</parental_control_level>
				<content_type type="string" />
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<clothing>
		<name type="string">Gothic Top Hat</name>
		<type type="string">Hat</type>
		<models type="list">
			<model type="bag">
				<name type="string">Male01_Hat_gothictophat</name>
				<lods type="bag">
					<lod1 type="string">Male01_Hat_gothictophat.lod1</lod1>
					<lod2 type="string">Male01_Hat_gothictophat.lod2</lod2>
					<lod3 type="string">Male01_Hat_gothictophat.lod3</lod3>
				</lods>
				<skeleton type="string">head</skeleton>
				<materials type="list">
					<material type="bag">
						<type type="string">default</type>
						<shadingGroup type="string">hatSG</shadingGroup>
						<path type="string" />
						<maps type="list">
							<map type="bag">
								<name type="string">colourMap</name>
								<source type="string">Male01_Hat_gothictophat.hatSG.colourMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">normalMap</name>
								<source type="string">Male01_Hat_gothictophat.hatSG.normalMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">specularMap</name>
								<source type="string">Male01_Hat_gothictophat.hatSG.specularMap</source>
								<format type="string">resource</format>
							</map>
						</maps>
					</material>
					<material type="bag">
						<type type="string">hair</type>
						<shadingGroup type="string">hairSG</shadingGroup>
						<path type="string" />
						<maps type="list">
							<map type="bag">
								<name type="string">colourMap</name>
								<source type="string">Male01_Hat_gothictophat.hairSG.colourMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">alphaMap</name>
								<source type="string">Male01_Hat_gothictophat.hairSG.alphaMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">specularMap</name>
								<source type="string">Male01_Hat_gothictophat.hairSG.specularMap</source>
								<format type="string">resource</format>
							</map>
						</maps>
					</material>
				</materials>
			</model>
		</models>
	</clothing>
