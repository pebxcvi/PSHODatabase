  <name lang="en-GB" default="true">Locomotion - Picnic Sheet (Camellia)</name>
  <name lang="fr-FR" default="false">Locomotion - Bâche pique-nique (camélias)</name>
  <name lang="de-DE" default="false">Bewegungsgegenstand - Picknickdecke (Kamelie)</name>
