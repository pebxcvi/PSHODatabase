		<object_version type="string">1.0</object_version>
		<company type="string">Sony Computer Entertainment Europe</company>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string">PEGI</rating_system>
				<rating type="string">16+</rating>
				<minimum_age type="int">16</minimum_age>
				<parental_control_level type="int">7</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
