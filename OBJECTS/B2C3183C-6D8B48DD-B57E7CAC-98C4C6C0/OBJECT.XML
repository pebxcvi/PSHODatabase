<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.3.2.16.BETA1" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">MotorStorm® Smoke Machine</name>
		<description type="string">MotorStorm® Smoke Machine</description>
