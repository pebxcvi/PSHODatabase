  <name lang="en-GB" default="true">The Bar Key</name>
  <name lang="fr-FR" default="false">La clé du bar</name>
  <name lang="de-DE" default="false">Der Barschlüssel</name>
  <name lang="it-IT" default="false">La chiave del bar</name>
  <name lang="es-ES" default="false">La llave del bar</name>
  <name lang="ja-JP" default="false">バーのカギ</name>
