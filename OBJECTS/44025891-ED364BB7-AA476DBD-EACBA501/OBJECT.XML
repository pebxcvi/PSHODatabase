<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.3.6.22.PUB" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<author type="string">John C. Ardussi</author>
		<object_version type="string">1.0</object_version>
		<company type="string">Sony Computer Entertainment America</company>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<furniture>
		<model type="string">model</model>
		<collision type="string">collision</collision>
		<appliance type="string">PlainObject</appliance>
		<camera_focus_offset type="vector">0 0 0</camera_focus_offset>
		<camera_distance type="float">0</camera_distance>
		<seats type="list" />
	</furniture>
	<lua_environment>
		<script type="string">RobotDog.lua</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string"></on_listener_received_data>
		<on_destroy type="string">OnDestroy()</on_destroy>
	</lua_environment>
	<targetable>
		<on_activated type="string">OnActivated()</on_activated>
		<on_targeted type="string">OnTartgeted()</on_targeted>
		<is_targetable_by_remote_people type="bool">true</is_targetable_by_remote_people>
		<trigger_radius type="float">-1</trigger_radius>
	</targetable>
	<entity />
	<network>
		<definition type="string">Network.xml</definition>
		<on_bag_created type="string"></on_bag_created>
		<on_bag_deleted type="string"></on_bag_deleted>
		<on_message_received type="string">OnReceivedMessage()</on_message_received>
		<auto_replicate type="bool">false</auto_replicate>
	</network>
	<renderer />
	<system />
	<active>
		<on_update type="string">OnFurnitureUpdate()</on_update>
		<upright_validation type="bool">true</upright_validation>
	</active>
</objectdef>