  <name lang="de-DE" default="false">Radecs Taktiker-Handschuhe</name>
  <name lang="it-IT" default="false">Guanti tattici di Radec</name>
  <name lang="es-ES" default="false">Guantes tácticos de Radec</name>
  <name lang="ko-KR" default="false">라덱 대령의 전술 장갑</name>
