<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">Directors Chair</name>
		<description type="string">Directors Chair</description>
		<author type="string">Richard Bisso</author>
		<object_version type="string">0.8</object_version>
		<company type="string">© 2009 Loot. All Rights Reserved.</company>
		<legal type="list">
			<age_rating type="bag">
				<country type="string" />
				<rating_system type="string" />
				<rating type="string" />
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">0</parental_control_level>
				<content_type type="string" />
				<additional_data type="string" />
			</age_rating>
		</legal>
	</header>
	<lua_environment>
		<script type="string">main.lua</script>
	</lua_environment>
	<entity />
	<network>
		<definition type="string">Network.xml</definition>
		<on_bag_created type="string">OnBagCreated()</on_bag_created>
		<on_bag_deleted type="string">OnBagDeleted()</on_bag_deleted>
		<on_message_received type="string">OnReceivedMessage()</on_message_received>
		<auto_replicate type="bool">true</auto_replicate>
	</network>
	<osd>
		<types type="string">types.xml</types>
	</osd>
	<mini_game>
		<on_update type="string">OnUpdate()</on_update>
		<on_send_welcome_message type="string">OnSendWelcomeMessage()</on_send_welcome_message>
		<on_render type="string">OnRender()</on_render>
		<on_can_local_player_join type="string">OnDecideIfLocalPlayerCanJoinGame()</on_can_local_player_join>
		<on_add_player type="string">OnAddPlayer()</on_add_player>
		<on_remove_player type="string">OnRemovePlayer()</on_remove_player>
		<on_local_player_join type="string">OnLocalPlayerJoin()</on_local_player_join>
		<on_local_player_leave type="string">OnLocalPlayerLeave()</on_local_player_leave>
		<on_local_player_update_gameplay type="string">OnLocalPlayerUpdateGamePlay()</on_local_player_update_gameplay>
		<join_type type="string">minimal</join_type>
	</mini_game>
	<pad />
	<renderer />
	<camera />
	<light />
</objectdef>