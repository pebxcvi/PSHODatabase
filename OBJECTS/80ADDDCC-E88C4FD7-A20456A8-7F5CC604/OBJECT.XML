<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.60.0.450.BETA" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<author type="string">mtsai</author>
		<object_version type="string">1.0</object_version>
		<company type="string">Sony Computer Entertainment</company>
		<template type="string"></template>
		<template_version type="string"></template_version>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<lua_environment>
		<script type="string">Main.lua</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string"></on_listener_received_data>
		<on_destroy type="string"></on_destroy>
		<on_clothing_change type="string"></on_clothing_change>
	</lua_environment>
	<entity />
	<renderer />
	<light>
		<lights type="list">
			<light type="bag">
				<offset type="vector">0 0 0</offset>
				<light_mask type="string">caustic_0000.dds</light_mask>
			</light>
		</lights>
	</light>
	<embedded_object>
		<on_update type="string">Home_onUpdate()</on_update>
		<string_parameters type="list" />
		<boolean_parameters type="list" />
		<numeric_parameters type="list">
			<parameter type="bag">
				<name type="string">power</name>
				<default_value type="float">0.8</default_value>
			</parameter>
			<parameter type="bag">
				<name type="string">fov</name>
				<default_value type="float">170</default_value>
				<min_value type="string">0</min_value>
				<max_value type="string">180</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">activation distance</name>
				<default_value type="float">20</default_value>
			</parameter>
			<parameter type="bag">
				<name type="string">length</name>
				<default_value type="float">8.5</default_value>
				<min_value type="string">0</min_value>
			</parameter>
			<parameter type="bag">
				<name type="string">direction</name>
				<default_value type="float">-1</default_value>
				<min_value type="string">-1</min_value>
				<max_value type="string">1</max_value>
			</parameter>
			<parameter type="bag">
				<name type="string">color.r</name>
				<default_value type="float">1.41</default_value>
				<min_value type="string">0</min_value>
			</parameter>
			<parameter type="bag">
				<name type="string">color.g</name>
				<default_value type="float">1.9</default_value>
				<min_value type="string">0</min_value>
			</parameter>
			<parameter type="bag">
				<name type="string">color.b</name>
				<default_value type="float">2.2</default_value>
				<min_value type="string">0</min_value>
			</parameter>
		</numeric_parameters>
		<enum_parameters type="list" />
	</embedded_object>
</objectdef>