  <name lang="fr-FR" default="false">Quartier Général de Men in Black™ - Clubhouse</name>
  <name lang="de-DE" default="false">Men in Black™ Hauptquartier - Clubhaus</name>
  <name lang="it-IT" default="false">Quartier generale Men in Black™ - Club House</name>
