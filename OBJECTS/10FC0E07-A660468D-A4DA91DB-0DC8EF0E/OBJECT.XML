			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000000</rig>
				<repertoire_resource type="string">male.xml</repertoire_resource>
				<emote_resource type="string"></emote_resource>
				<label type="string">male.xml</label>
			</target>
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000001</rig>
				<repertoire_resource type="string">female.xml</repertoire_resource>
				<emote_resource type="string"></emote_resource>
				<label type="string">female.xml</label>
			</target>
		</target_rigs>
