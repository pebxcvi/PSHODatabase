<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.60.0.451.PUB" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<author type="string">hongchen</author>
		<object_version type="string">1.0</object_version>
		<company type="string">Sony Computer Entertainment</company>
		<template type="string">active_minigame</template>
		<template_version type="string">1.1.0</template_version>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<furniture>
		<model type="string">runner_01.mdl</model>
		<collision type="string">runner_01.hkx</collision>
		<appliance type="string">PlainObject</appliance>
		<camera_focus_offset type="vector">0 0 0</camera_focus_offset>
		<camera_distance type="float">0</camera_distance>
		<seats type="list" />
	</furniture>
	<lua_environment>
		<script type="string">runner.lua</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string">OnListenerReceivedData()</on_listener_received_data>
		<on_destroy type="string">OnDestroy()</on_destroy>
		<on_clothing_change type="string"></on_clothing_change>
	</lua_environment>
	<entity />
	<network>
		<definition type="string">Network.xml</definition>
		<on_bag_created type="string"></on_bag_created>
		<on_bag_deleted type="string"></on_bag_deleted>
		<on_message_received type="string"></on_message_received>
		<auto_replicate type="bool">false</auto_replicate>
	</network>
	<mini_game>
		<on_update type="string">OnUpdate()</on_update>
		<on_send_welcome_message type="string">OnSendWelcomeMessage()</on_send_welcome_message>
		<on_render type="string">OnRender()</on_render>
		<on_can_local_player_join type="string">OnCanLocalPlayerJoin()</on_can_local_player_join>
		<on_add_player type="string">OnAddPlayer()</on_add_player>
		<on_remove_player type="string">OnRemovePlayer()</on_remove_player>
		<on_local_player_join type="string">OnLocalPlayerJoin()</on_local_player_join>
		<on_local_player_leave type="string">OnLocalPlayerLeave()</on_local_player_leave>
		<on_local_player_update_gameplay type="string">OnLocalPlayerUpdateGameplay()</on_local_player_update_gameplay>
		<join_type type="string">normal</join_type>
		<is_allowed_label_change type="bool">false</is_allowed_label_change>
		<is_allowed_chat_bubble_change type="bool">false</is_allowed_chat_bubble_change>
		<session_size type="int">1</session_size>
		<network_session type="bool">true</network_session>
		<on_custom_exit_menu_option_selected type="string"></on_custom_exit_menu_option_selected>
	</mini_game>
	<pad />
	<repertoire>
		<is_exclusive type="bool">false</is_exclusive>
		<target_rigs type="list">
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000000</rig>
				<repertoire_resource type="string">MaleRep</repertoire_resource>
				<emote_resource type="string"></emote_resource>
				<label type="string">Runner_Male_Label</label>
			</target>
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000001</rig>
				<repertoire_resource type="string">FemaleRep</repertoire_resource>
				<emote_resource type="string"></emote_resource>
				<label type="string">Runner_Female_Label</label>
			</target>
		</target_rigs>
	</repertoire>
	<game_spawner>
		<game_type type="string">mini_game</game_type>
		<attached_to_furniture type="bool">true</attached_to_furniture>
		<use_default_camera type="bool">true</use_default_camera>
		<local_person_visible type="bool">false</local_person_visible>
		<remote_people_visible type="bool">false</remote_people_visible>
		<remote_portables_visible type="bool">true</remote_portables_visible>
		<mini_game type="bag" />
	</game_spawner>
	<active>
		<upright_validation type="bool">true</upright_validation>
		<safe_volume type="string">runner_01.safevolume.hkx</safe_volume>
	</active>
</objectdef>