<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.60.0.450.BETA" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<author type="string">Lockwood</author>
		<object_version type="string">1.0</object_version>
		<company type="string">Lockwood</company>
		<template type="string">active_minigame</template>
		<template_version type="string">1.1.0</template_version>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
			<age_rating type="bag">
				<country type="string">US</country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">13</minimum_age>
				<parental_control_level type="int">0</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
			<age_rating type="bag">
				<country type="string">CA</country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">13</minimum_age>
				<parental_control_level type="int">0</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
			<age_rating type="bag">
				<country type="string">KR</country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">3</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<furniture>
		<model type="string">Base.mdl</model>
		<collision type="string">Base.hkx</collision>
		<appliance type="string">PlainObject</appliance>
		<camera_focus_offset type="vector">0 1 0</camera_focus_offset>
		<camera_distance type="float">2.5</camera_distance>
		<seats type="list" />
	</furniture>
	<lua_environment>
		<script type="string">main.lua</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string">main.Listen()</on_listener_received_data>
		<on_destroy type="string">main.Destroy()</on_destroy>
		<on_clothing_change type="string"></on_clothing_change>
	</lua_environment>
	<entity />
	<particles />
	<screen />
	<pad />
	<renderer />
	<camera />
	<system />
	<light />
	<active>
		<on_update type="string">main.Update()</on_update>
		<upright_validation type="bool">true</upright_validation>
		<safe_volume type="string">Base.safevolume.hkx</safe_volume>
	</active>
</objectdef>