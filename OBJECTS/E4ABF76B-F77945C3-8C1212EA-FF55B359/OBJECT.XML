<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.65.0.139.BETA" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<help_text type="string"></help_text>
		<author type="string">Lockwood</author>
		<object_version type="string">1.0</object_version>
		<company type="string"></company>
		<template type="string">scene_minigame</template>
		<template_version type="string">1.1.0</template_version>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<lua_environment>
		<script type="string">Toolbox.lua</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string">*OnIntergameRecieved</on_listener_received_data>
		<on_destroy type="string"></on_destroy>
		<on_clothing_change type="string"></on_clothing_change>
	</lua_environment>
	<targetable>
		<interactive type="bool">true</interactive>
		<on_activated type="string"></on_activated>
		<on_targeted type="string"></on_targeted>
		<is_targetable_by_remote_people type="bool">false</is_targetable_by_remote_people>
		<trigger_radius type="float">-1</trigger_radius>
		<can_be_targeted_while_seated type="bool">false</can_be_targeted_while_seated>
	</targetable>
	<entity />
	<particles />
	<network>
		<definition type="string">Network.xml</definition>
		<on_bag_created type="string"></on_bag_created>
		<on_bag_deleted type="string"></on_bag_deleted>
		<on_message_received type="string">*OnNetReceived</on_message_received>
		<auto_replicate type="bool">false</auto_replicate>
	</network>
	<mini_game>
		<on_update type="string">*Update</on_update>
		<on_send_welcome_message type="string"></on_send_welcome_message>
		<on_render type="string">*Render</on_render>
		<on_can_local_player_join type="string">*CanJoin</on_can_local_player_join>
		<on_add_player type="string">*RemoteJoin</on_add_player>
		<on_remove_player type="string">*RemoteLeave</on_remove_player>
		<on_local_player_join type="string">*Join</on_local_player_join>
		<on_local_player_leave type="string">*Leave</on_local_player_leave>
		<on_local_player_update_gameplay type="string">*LocalUpdate</on_local_player_update_gameplay>
		<join_type type="string">minimal</join_type>
		<is_allowed_label_change type="bool">true</is_allowed_label_change>
		<is_allowed_chat_bubble_change type="bool">true</is_allowed_chat_bubble_change>
		<session_size type="int">64</session_size>
		<network_session type="bool">true</network_session>
		<on_custom_exit_menu_option_selected type="string"></on_custom_exit_menu_option_selected>
	</mini_game>
	<screen />
	<pad />
	<renderer />
	<camera />
	<system />
	<light />
	<repertoire>
		<is_exclusive type="bool">false</is_exclusive>
		<target_rigs type="list">
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000000</rig>
				<repertoire_resource type="string">male</repertoire_resource>
				<emote_resource type="string"></emote_resource>
				<label type="string">male</label>
			</target>
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000001</rig>
				<repertoire_resource type="string">female</repertoire_resource>
				<emote_resource type="string"></emote_resource>
				<label type="string">female</label>
			</target>
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000000</rig>
				<repertoire_resource type="string">feedMale</repertoire_resource>
				<emote_resource type="string"></emote_resource>
				<label type="string">feedMale</label>
			</target>
			<target type="bag">
				<rig type="string">00000000-00000000-00000010-00000001</rig>
				<repertoire_resource type="string">feedFemale</repertoire_resource>
				<emote_resource type="string"></emote_resource>
				<label type="string">feedFemale</label>
			</target>
		</target_rigs>
		<anim_registers type="bag">
			<max_players type="int">0</max_players>
			<string_registers type="list" />
			<bool_registers type="list" />
			<number_registers type="list" />
			<integer_registers type="list" />
		</anim_registers>
	</repertoire>
	<embedded_object>
		<on_update type="string"></on_update>
		<string_parameters type="list" />
		<boolean_parameters type="list" />
		<numeric_parameters type="list">
			<parameter type="bag">
				<name type="string">max_load_time</name>
				<default_value type="float">30</default_value>
				<min_value type="string">0</min_value>
				<max_value type="string">99999</max_value>
			</parameter>
		</numeric_parameters>
		<enum_parameters type="list" />
	</embedded_object>
</objectdef>