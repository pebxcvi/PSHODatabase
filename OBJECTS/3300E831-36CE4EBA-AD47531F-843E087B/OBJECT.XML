		<is_allowed_label_change type="bool">false</is_allowed_label_change>
		<is_allowed_chat_bubble_change type="bool">false</is_allowed_chat_bubble_change>
		<session_size type="int">2</session_size>
		<network_session type="bool">true</network_session>
	</mini_game>
	<pad />
	<game_spawner>
		<game_type type="string">mini_game</game_type>
		<attached_to_furniture type="bool">true</attached_to_furniture>
		<use_default_camera type="bool">true</use_default_camera>
		<local_person_visible type="bool">false</local_person_visible>
		<remote_people_visible type="bool">false</remote_people_visible>
		<remote_portables_visible type="bool">true</remote_portables_visible>
