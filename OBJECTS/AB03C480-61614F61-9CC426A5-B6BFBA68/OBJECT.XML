<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.41.0.37.PUB" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<author type="string">The Odd Gentlemen</author>
		<object_version type="string">1.0</object_version>
		<company type="string">Codename Games</company>
		<template type="string"></template>
		<template_version type="string"></template_version>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<furniture>
		<model type="string">placement_model</model>
		<collision type="string">collision</collision>
		<appliance type="string">PlainObject</appliance>
		<camera_focus_offset type="vector">0 0.5 0</camera_focus_offset>
		<camera_distance type="float">3</camera_distance>
		<seats type="list" />
	</furniture>
	<lua_environment>
		<script type="string">script</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string"></on_listener_received_data>
		<on_destroy type="string"></on_destroy>
	</lua_environment>
	<targetable>
		<interactive type="bool">false</interactive>
		<on_activated type="string">CallMe()</on_activated>
		<on_targeted type="string"></on_targeted>
		<is_targetable_by_remote_people type="bool">true</is_targetable_by_remote_people>
		<trigger_radius type="float">-1</trigger_radius>
	</targetable>
	<entity />
	<network>
		<definition type="string"></definition>
		<on_bag_created type="string"></on_bag_created>
		<on_bag_deleted type="string"></on_bag_deleted>
		<on_message_received type="string">OnMessage()</on_message_received>
		<auto_replicate type="bool">false</auto_replicate>
	</network>
	<active>
		<on_update type="string">Update()</on_update>
		<upright_validation type="bool">false</upright_validation>
	</active>
</objectdef>