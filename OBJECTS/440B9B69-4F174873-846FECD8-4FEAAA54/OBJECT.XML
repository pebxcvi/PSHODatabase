		<on_activated type="string">OnActivated()</on_activated>
		<on_targeted type="string">OnTargeted()</on_targeted>
		<is_targetable_by_remote_people type="bool">true</is_targetable_by_remote_people>
		<trigger_radius type="float">-1</trigger_radius>
		<can_be_targeted_while_seated type="bool">false</can_be_targeted_while_seated>
	</targetable>
	<entity />
	<active>
		<on_update type="string">OnUpdate()</on_update>
		<upright_validation type="bool">true</upright_validation>
	</active>
