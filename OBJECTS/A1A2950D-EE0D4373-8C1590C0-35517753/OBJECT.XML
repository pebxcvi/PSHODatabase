<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.75.0.85.PUB" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<help_text type="string"></help_text>
		<author type="string">neil</author>
		<object_version type="string">1.0.1</object_version>
		<company type="string">Sony Computer Entertainment Europe</company>
		<template type="string"></template>
		<template_version type="string"></template_version>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string"></rating_system>
				<rating type="string"></rating>
				<minimum_age type="int">1</minimum_age>
				<parental_control_level type="int">0</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
		<LPID type="string"></LPID>
	</header>
	<lua_environment>
		<script type="string">VMMiniGame_lua</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string"></on_listener_received_data>
		<on_destroy type="string"></on_destroy>
		<on_clothing_change type="string"></on_clothing_change>
		<senders type="list" />
	</lua_environment>
	<entity />
	<network>
		<definition type="string">Network_XML</definition>
		<on_bag_created type="string"></on_bag_created>
		<on_bag_deleted type="string"></on_bag_deleted>
		<on_message_received type="string"></on_message_received>
		<auto_replicate type="bool">false</auto_replicate>
	</network>
	<mini_game>
		<on_update type="string">OnUpdate()</on_update>
		<on_send_welcome_message type="string"></on_send_welcome_message>
		<on_render type="string">OnRender()</on_render>
		<on_can_local_player_join type="string">OnDecideIfLocalPlayerCanJoinGame()</on_can_local_player_join>
		<on_add_player type="string"></on_add_player>
		<on_remove_player type="string"></on_remove_player>
		<on_local_player_join type="string">OnLocalPlayerJoin()</on_local_player_join>
		<on_local_player_leave type="string"></on_local_player_leave>
		<on_local_player_update_gameplay type="string"></on_local_player_update_gameplay>
		<join_type type="string">minimal</join_type>
		<is_allowed_label_change type="bool">false</is_allowed_label_change>
		<is_allowed_chat_bubble_change type="bool">false</is_allowed_chat_bubble_change>
		<session_size type="int">2</session_size>
		<network_session type="bool">true</network_session>
		<on_custom_exit_menu_option_selected type="string"></on_custom_exit_menu_option_selected>
	</mini_game>
	<renderer />
</objectdef>