<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.52.0.2.PUB" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">kName</name>
		<description type="string">kDesc</description>
		<author type="string">jposey</author>
		<object_version type="string">1.0</object_version>
		<company type="string">Hellfire Games, Inc.</company>
		<template type="string">active</template>
		<template_version type="string">1.0.0</template_version>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string">PEGI</rating_system>
				<rating type="string">7+</rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string">PEGI_5</content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<furniture>
		<model type="string">ActiveModel.mdl</model>
		<collision type="string">ActiveModel.hkx</collision>
		<appliance type="string">PlainObject</appliance>
		<camera_focus_offset type="vector">0 0 0</camera_focus_offset>
		<camera_distance type="float">0</camera_distance>
		<seats type="list" />
	</furniture>
	<lua_environment>
		<script type="string">Active.lua</script>
		<debug type="bool">false</debug>
		<on_listener_received_data type="string">OnListenerReceivedData()</on_listener_received_data>
		<on_destroy type="string">OnDestroy()</on_destroy>
	</lua_environment>
	<targetable>
		<interactive type="bool">true</interactive>
		<on_activated type="string">OnActivated()</on_activated>
		<on_targeted type="string"></on_targeted>
		<is_targetable_by_remote_people type="bool">true</is_targetable_by_remote_people>
		<trigger_radius type="float">-1</trigger_radius>
	</targetable>
	<osd>
		<types type="string">types_stereo.xml</types>
	</osd>
	<mini_game>
		<on_update type="string">OnUpdate()</on_update>
		<on_send_welcome_message type="string"></on_send_welcome_message>
		<on_render type="string"></on_render>
		<on_can_local_player_join type="string"></on_can_local_player_join>
		<on_add_player type="string"></on_add_player>
		<on_remove_player type="string"></on_remove_player>
		<on_local_player_join type="string">OnLocalPlayerJoin()</on_local_player_join>
		<on_local_player_leave type="string">OnLocalPlayerLeave()</on_local_player_leave>
		<on_local_player_update_gameplay type="string">OnUpdateGameplay()</on_local_player_update_gameplay>
		<join_type type="string">minimal</join_type>
		<is_allowed_label_change type="bool">false</is_allowed_label_change>
		<is_allowed_chat_bubble_change type="bool">false</is_allowed_chat_bubble_change>
		<session_size type="int">64</session_size>
		<network_session type="bool">true</network_session>
	</mini_game>
	<pad />
	<active>
		<on_update type="string">OnActiveUpdate()</on_update>
		<upright_validation type="bool">true</upright_validation>
		<safe_volume type="string">safe_volume</safe_volume>
	</active>
</objectdef>