<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">name</name>
		<description type="string">name</description>
