<?xml version="1.0" encoding="utf-8"?>
<objectdef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" hdk_version="1.3.2.16.BETA1" file_version="1" xmlns="http://home.scedev.net/schema/object">
	<header>
		<name type="string">Name</name>
		<description type="string">Description</description>
		<author type="string">Joost Peters</author>
		<object_version type="string">1.0</object_version>
		<company type="string">The Original V-Shirt Store</company>
		<legal type="list">
			<age_rating type="bag">
				<country type="string"></country>
				<rating_system type="string">PEGI</rating_system>
				<rating type="string">3+</rating>
				<minimum_age type="int">0</minimum_age>
				<parental_control_level type="int">1</parental_control_level>
				<content_type type="string"></content_type>
				<additional_data type="string"></additional_data>
			</age_rating>
		</legal>
	</header>
	<clothing>
		<name type="string">Name</name>
		<type type="string">Tors</type>
		<models type="list">
			<model type="bag">
				<name type="string">Male01_Torso_Yoko_Ono</name>
				<lods type="bag">
					<lod1 type="string">Male01_Torso_Yoko_Ono.lod1</lod1>
					<lod2 type="string">Male01_Torso_Yoko_Ono.lod2</lod2>
					<lod3 type="string">Male01_Torso_Yoko_Ono.lod3</lod3>
				</lods>
				<skeleton type="string"></skeleton>
				<materials type="list">
					<material type="bag">
						<type type="string">default</type>
						<shadingGroup type="string">torsoSG</shadingGroup>
						<path type="string"></path>
						<maps type="list">
							<map type="bag">
								<name type="string">colourMap</name>
								<source type="string">Male01_Torso_Yoko_Ono.torsoSG.colourMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">normalMap</name>
								<source type="string">Male01_Torso_Yoko_Ono.torsoSG.normalMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">specularMap</name>
								<source type="string">Male01_Torso_Yoko_Ono.torsoSG.specularMap</source>
								<format type="string">resource</format>
							</map>
						</maps>
					</material>
					<material type="bag">
						<type type="string">skin</type>
						<shadingGroup type="string">bodySG</shadingGroup>
						<path type="string"></path>
						<maps type="list">
							<map type="bag">
								<name type="string">colourMap</name>
								<source type="string">MaleSkinBodyColour</source>
								<format type="string">generated</format>
							</map>
							<map type="bag">
								<name type="string">normalMap</name>
								<source type="string">Male01_Torso_Yoko_Ono.bodySG.normalMap</source>
								<format type="string">resource</format>
							</map>
							<map type="bag">
								<name type="string">specularMap</name>
								<source type="string">MaleSkinBodySpecular</source>
								<format type="string">generated</format>
							</map>
						</maps>
					</material>
				</materials>
			</model>
		</models>
	</clothing>
</objectdef>