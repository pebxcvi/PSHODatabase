  <name lang="en-GB" default="true">Imperial Snowtrooper Suit</name>
  <name lang="fr-FR" default="false">Combinaison Snowtrooper Impérial</name>
  <name lang="de-DE" default="false">Imperialer Schneetruppen-Anzug</name>
  <name lang="it-IT" default="false">Costume Snowtrooper Imperiale</name>
  <name lang="es-ES" default="false">Traje de Snowtrooper imperial</name>
  <name lang="ja-JP" default="false">帝国軍スノー・トルーパー・スーツ</name>
  <name lang="ko-KR" default="false">제국 스노우트루퍼 수트</name>
